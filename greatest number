#include <stdio.h>
int a, b, c;
int main() {
    printf("Enter numbers for a, b, c: ");
    scanf("%d %d %d", &a, &b, &c);
    if (a > b && a > c)
    {
      printf("The greatest number is %d",a,b,c);
    }
        else
        {
            printf("default");
            return 0;
        }
    }
