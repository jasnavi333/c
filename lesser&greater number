#include <stdio.h>
int=40;
int main()
{
   if(n=10)
   {
      printf("the number is equal to 10");
   }
  elseif(n>10)
   {
      printf("the number is greaterthan 10");
   }
   elseif(n<10)
     {
        printf("the number is lessthan 10");
     }
     else
       {
           printf(" the number is zero");
       }
}
